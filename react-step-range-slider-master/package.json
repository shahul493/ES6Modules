{
  "name": "react-step-range-slider",
  "description": "A range slider input for React",
  "main": "dist/index.js",
  "version": "1.4.1",
  "author": "Robert Gonzales",
  "license": "MIT",
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "es6",
    "breakpoint",
    "slider",
    "range",
    "input",
    "steps",
    "step"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/robertgonzales/react-step-range-slider.git"
  },
  "bugs": "https://github.com/robertgonzales/react-step-range-slider/issues",
  "scripts": {
    "clean:build": "node ./bin/clean.js createdir",
    "build:html": "node ./bin/buildHtml.js",
    "prestart": "node ./bin/clean.js",
    "start": "webpack-dev-server --config ./config/webpack.config.dev.js --hot --inline --progress",
    "prebuild": "npm run clean:build && npm run build:html",
    "build": "export NODE_ENV=production & NODE_ENV=production & webpack -p --config ./config/webpack.config.prod.js --progress",
    "prepublish": "babel --plugins=transform-es2015-modules-umd,transform-class-properties src --out-dir ./dist"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "6.7.7",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
    "copy-webpack-plugin": "2.1.3",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "node-sass": "^3.7.0",
    "postcss-loader": "0.8.2",
    "react-addons-test-utils": "15.0.1",
    "react-hot-loader": "1.3.0",
    "rimraf": "2.5.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.14.1"
  },
  "peerDependencies": {
    "react": "^15",
    "react-dom": "^15"
  },
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "classnames": "^2.2.5",
    "lodash": "^4.11.1",
    "prop-types": "^15"
  }
}
